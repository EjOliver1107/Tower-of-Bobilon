<%- include('../partials/header') %>
<table>
    <thead>
        <tr>
            <th>Drink</th>
        </tr>
    </thead>
    <tbody>
        <% drinks.forEach(function(d){ %>
         <tr>
             <td><%= d.title %></td>
             <td><%= d.img %></td>
             <td><a href="/drinks/<%= d._id %>">Details</a></td>
         </tr>
        <% }) %>
    </tbody>
</table>
<%- include('../partials/footer') %>